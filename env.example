# /.env.example

# --- API Keys ---
# 各LLMプロバイダーのAPIキーを設定
OPENAI_API_KEY="sk-..."
CLAUDE_API_KEY="sk-ant-..."
GEMINI_API_KEY="AIza..."
HF_TOKEN="hf_..." # Hugging Face Hub API Token
SERPAPI_API_KEY="your_serpapi_api_key_here" #SerpApi service

# --- Provider Configuration ---
# 各プロバイダーで使用するデフォルトのモデル名を指定
OPENAI_DEFAULT_MODEL="gpt-4-turbo"
CLAUDE_DEFAULT_MODEL="claude-3-sonnet-20240229"
GEMINI_DEFAULT_MODEL="gemini-1.5-flash"
OLLAMA_DEFAULT_MODEL="gemma3"

# --- CogniQuantum Engine Configuration ---
# CogniQuantumシステムの動作を制御
COGNIQUANTUM_ENABLED="true" # 全体的な有効/無効
COGNIQUANTUM_COMPLEXITY_THRESHOLD="1.0" # この閾値を超えると自動的に拡張機能が作動
COGNIQUANTUM_DEFAULT_MODE="adaptive_thinking" # デフォルトの推論モード
COGNIQUANTUM_LEARNING_ENABLED="true" # インタラクションからの学習機能
COGNIQUANTUM_MONITORING_ENABLED="true" # パフォーマンス監視機能

# --- Logging Configuration ---
# ログレベル (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL="INFO"
